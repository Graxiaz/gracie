# Clone the repository
git clone https://github.com/Graxiaz/gracie.git

# Navigate to the repository folder
cd gracie

# Copy your HTML file into the repository folder
cp "C:\Users\Gregoria B Paredes\OneDrive\Desktop\ECPE101\updatedprofile.html"

# Add the file to the staging area
git add updatedprofile.html

# Commit the changes
git commit -m "Add updated profile HTML file"

# Push the changes to GitHub
git push origin main
